<div class="row">
  <div class="col">
  {{multi-select elements = model.dispoItems
               selectedIds = model.selectedDispoItemsIds}}
  </div>
  <div class="col-1">
    <button {{action "addToIncluded" model.selectedDispoItems}} class="btn btn-primary btn-fluid">
      {{fa-icon}}
    </button>
  </div>
  <div class="col">
  {{multi-select elements = model.includedItems
               selectedIds = model.selectedIncludedItemsIds}}
  </div>
</div>